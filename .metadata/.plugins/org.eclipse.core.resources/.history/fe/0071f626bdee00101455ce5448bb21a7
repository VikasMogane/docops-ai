server:
  port: 8081

spring:
  datasource:
    url: jdbc:postgresql://localhost:5432/docops?options=-c%20TimeZone=UTC
    username: docops
    password: docops
  jpa:
    hibernate:
      ddl-auto: update
    show-sql: true
    properties:
      hibernate:
        format_sql: true
            hibernate.jdbc.time_zone: UTC
      
jwt:
  secret: docops-secret-key-very-long
  expiry: 86400000 # 1 day
